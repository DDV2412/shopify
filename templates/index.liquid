<script>
  document.addEventListener("DOMContentLoaded", () => {
    const defaultPropertyType = document.getElementById("property-type-buy");
    const residentialRadio = document.getElementById("Residential");
    const propertySelected = document.getElementById("property-selected");
    propertySelected.textContent = residentialRadio.value;
    residentialRadio.checked = true;
    residentialRadio.previousElementSibling.classList.add('w--redirected-checked');
    residentialRadio.nextElementSibling.classList.add('is-active');
    defaultPropertyType.checked = true;
    defaultPropertyType.previousElementSibling.classList.add('w--redirected-checked');
    const dropdownList = document.getElementById("property-dropdown-nav");
    const dropdownToggle = document.getElementById("property-dropdown-select");
    const dowpdownContent = document.getElementById("property-dropdown-content");
    const propertyListing = document.querySelectorAll(".propert-radio");
    propertyListing.forEach((item) => {
      item.addEventListener('click', () => {
        const radioInput = item.querySelector('input[type="radio"]');
        const typeLabel = item.querySelector('.type-label');
        if (radioInput) {
          propertySelected.textContent = radioInput.value;
          dropdownList.classList.remove('w--open');
          dropdownToggle.classList.remove('w--open');
          dropdownToggle.setAttribute('aria-expanded', 'false');
          dowpdownContent.style = "";
          document.querySelectorAll('.type-label').forEach(label => {
            label.classList.remove('is-active');
          });
          typeLabel.classList.add('is-active');
        }
      });
    });
  });
</script>