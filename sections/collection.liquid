<script>
    document.addEventListener("DOMContentLoaded", () => {
        let searchValue = '';
        let typeValue = '';
        let minPrice = '';
        let maxPrice = '';
        let bedrooms = '';
        let bathrooms = '';

        const itemsPerPage = 20;
        let currentPage = 1;
        let totalPages = 1;

        const searchInput = document.getElementById('search-query');

        function applyFilters(searchValue, typeValue, minPrice, maxPrice, bedrooms, bathrooms) {
            let propertyCount = 0;

            const properties = document.querySelectorAll('.property-card-wrapper');
            properties.forEach(property => {
                const price = parseFloat(property.querySelector('#property-price').textContent.replace(/[^0-9.-]+/g, ""));
                const type = property.querySelector('#property-type').textContent.toLowerCase();
                const propertyBedrooms = parseInt(property.querySelector('#property-bedroom').textContent);
                const propertyBathrooms = parseInt(property.querySelector('#property-bathroom').textContent);
                const titleText = property.querySelector('#property-title').textContent.toLowerCase();

                const isSearchValueValid = searchValue !== '' ? titleText.includes(searchValue.toLowerCase()) : true;
                const isTypeValueValid = typeValue !== '' ? type === typeValue.toLowerCase() : true;
                const isMinPriceValid = minPrice !== '' ? price >= minPrice : true;
                const isMaxPriceValid = maxPrice !== '' ? price <= maxPrice : true;
                const isBedroomsValid = bedrooms !== '' ? propertyBedrooms >= bedrooms : true;
                const isBathroomsValid = bathrooms !== '' ? propertyBathrooms >= bathrooms : true;

                if (
                    isSearchValueValid &&
                    isTypeValueValid &&
                    isMinPriceValid &&
                    isMaxPriceValid &&
                    isBedroomsValid &&
                    isBathroomsValid
                ) {
                    property.classList.remove('product-hidden');
                    propertyCount++;
                } else {
                    property.classList.add('product-hidden');
                }
            });

            const propertyCountElement = document.getElementById('property-count');
            propertyCountElement.innerHTML = propertyCount + ' Properties';

            paginateProperties();

        }

        function paginateProperties() {
            const properties = document.querySelectorAll('.property-card-wrapper:not(.product-hidden)');
            const totalProperties = properties.length;
            totalPages = Math.ceil(totalProperties / itemsPerPage);

            properties.forEach((property, index) => {
                if (index >= (currentPage - 1) * itemsPerPage && index < currentPage * itemsPerPage) {
                    property.style.display = 'grid';
                } else {
                    property.style.display = 'none';
                }
            });

            updatePaginationDisplay();
        }

        function updatePaginationDisplay() {
            const pagesDropdown = document.getElementById('pagesDropdown');
            const currentPageDisplay = document.getElementById('currentPageDisplay');
            const totalPagesElement = document.getElementById('totalPages');

            currentPageDisplay.textContent = currentPage;

            totalPagesElement.textContent = totalPages;

            pagesDropdown.innerHTML = '';
            for (let i = 1; i <= totalPages; i++) {
                const pageLink = document.createElement('a');
                pageLink.href = "#";
                pageLink.classList.add('page-count', 'w-dropdown-link');
                pageLink.textContent = i;
                pageLink.addEventListener('click', (e) => {
                    e.preventDefault();
                    goToPage(i);
                });
                pagesDropdown.appendChild(pageLink);
            }
        }

        function goToPage(page) {
            if (page >= 1 && page <= totalPages) {
                currentPage = page;
                paginateProperties();
            }
        }

        document.getElementById('nextPage').addEventListener('click', (e) => {
            e.preventDefault();
            if (currentPage < totalPages) {
                currentPage++;
                paginateProperties();
            }
        });

        document.getElementById('prevPage').addEventListener('click', (e) => {
            e.preventDefault();
            if (currentPage > 1) {
                currentPage--;
                paginateProperties();
            }
        });

        applyFilters(searchValue, typeValue, minPrice, maxPrice, bedrooms, bathrooms);

        searchInput.addEventListener('input', (e) => {
            searchValue = e.target.value;
            applyFilters(searchValue, typeValue, minPrice, maxPrice, bedrooms, bathrooms);
        });

        document.getElementById('mobile-filter').addEventListener('submit', function (event) {
            event.preventDefault();

            const searchValue = document.getElementById('search-input-mobile').value;
            const productType = document.querySelector('input[name="filter.p.product_type"]:checked')?.value;
            const minPrice = document.querySelector('input[name="filter.v.price.gte"]:checked')?.value;
            const maxPrice = document.querySelector('input[name="filter.v.price.lte"]:checked')?.value;
            const bedrooms = document.querySelector('input[name="filter.v.option.betroom"]:checked')?.value;
            const bathrooms = document.querySelector('input[name="filter.v.option.bathroom"]:checked')?.value;

            applyFilters(searchValue, productType, minPrice, maxPrice, bedrooms, bathrooms);

        });

        const mostRecentRadio = document.getElementById("Most-Recent");
        const propertySelected = document.getElementById("property-selected");
        mostRecentRadio.checked = true;
        mostRecentRadio.previousElementSibling.classList.add('w--redirected-checked');
        mostRecentRadio.nextElementSibling.classList.add('is-active');

        const propertyListing = document.querySelectorAll(".dropdown-filter");

        const query = new URLSearchParams(window.location.search);
        const sortData = query.get('sort_by');

        const sortDataRadio = document.querySelectorAll('.sort-data');

        if (sortData) {
            sortDataRadio.forEach((radio) => {
                const radioInput = radio.querySelector('input[type="radio"]');
                const typeLabel = radio.querySelector('.type-label');
                if (radioInput.value === sortData) {
                    radioInput.checked = true;
                    radioInput.previousElementSibling.classList.add('w--redirected-checked');
                    radioInput.nextElementSibling.classList.add('is-active');
                    propertySelected.textContent = typeLabel.innerHTML;
                } else {
                    radioInput.checked = false;
                    radioInput.previousElementSibling.classList.remove('w--redirected-checked');
                    radioInput.nextElementSibling.classList.remove('is-active');
                }
            });
        }
        propertyListing.forEach((item) => {
            item.addEventListener('click', () => {
                const propertySelected = item.querySelectorAll(".propert-radio");
                const propertySelectedName = item.querySelector(".property-selected");
                const dropdownList = item.querySelector('.property-list');
                const dropdownToggle = item.querySelector('.property-select');
                const dropdownContent = item;

                propertySelected.forEach((radio) => {
                    radio.addEventListener('change', () => {
                        const radioInput = radio.querySelector('input[type="radio"]');
                        const typeLabel = radio.querySelector('.type-label');
                        if (radioInput) {
                            propertySelected.forEach((listing) => {
                                const siblingRadio = listing.querySelector('input[type="radio"]');
                                if (siblingRadio) {
                                    siblingRadio.previousElementSibling.classList.remove('w--redirected-checked');
                                    siblingRadio.nextElementSibling.classList.remove('is-active');
                                }
                            });

                            propertySelectedName.textContent = typeLabel.innerHTML;
                            radioInput.previousElementSibling.classList.add('w--redirected-checked');
                            radioInput.nextElementSibling.classList.add('is-active');

                            if (radio.className.includes('sort-data')) {
                                const sortData = radioInput.value;
                                query.set('sort_by', sortData);
                                window.location.search = query.toString();
                            } else {
                                if (radioInput.name === 'filter.v.price.gte') {
                                    minPrice = radioInput.value;
                                } else if (radioInput.name === 'filter.v.price.lte') {
                                    maxPrice = radioInput.value;
                                } else if (radioInput.name === 'filter.v.option.betroom') {
                                    bedrooms = radioInput.value;
                                } else if (radioInput.name === 'filter.v.option.bathroom') {
                                    bathrooms = radioInput.value;
                                } else if (radioInput.name === 'filter.p.product_type') {
                                    typeValue = radioInput.value;
                                }

                                applyFilters(searchValue, typeValue, minPrice, maxPrice, bedrooms, bathrooms);
                            }

                            dropdownList.classList.remove('w--open');
                            dropdownToggle.classList.remove('w--open');
                            dropdownToggle.setAttribute('aria-expanded', 'false');
                            dropdownContent.style = "";
                        }
                    });
                });
            });
        });

    });
</script>

