<section class="section_component">
  <div class="container">
    <div
      data-w-id="d9f7959d-a9b5-9387-cac8-6e7c88cfaecc"
      style="opacity: 1; transform: translate3d(0px, 0px, 0px) scale3d(1, 1, 1) rotateX(0deg) rotateY(0deg) rotateZ(0deg) skew(0deg, 0deg); transform-style: preserve-3d;"
      class="cta-wrapper">
      <div id="w-node-d9f7959d-a9b5-9387-cac8-6e7c88cfaecd-732b4aee" class="cta-content">
        {%- for block in section.blocks -%}
          {%- case block.type -%}
            {%- when 'heading' -%}
              <div class="cta-title">

                <h2>{{ block.settings.heading }}</h2>
                <div class="cta-tag">{{ block.settings.tagline }}</div>
              </div>
            {%- when 'text' -%}
              <div>{{ block.settings.text }}</div>
            {%- when 'button' -%}
              <div class="button-wrapper padding-top-large align-left">
                <a href="{{ block.settings.cta_link }}" class="button is-light w-inline-block">
                  <div>{{ block.settings.cta_button }}</div>
                </a>
              </div>
          {%- endcase -%}
        {%- endfor -%}
      </div>
      <div id="w-node-d9f7959d-a9b5-9387-cac8-6e7c88cfaeda-732b4aee" class="cta-image">
        <div id="w-node-d9f7959d-a9b5-9387-cac8-6e7c88cfaedb-732b4aee" class="cta_image-wrapper _1">
          {% for product in collections.all.products %}
            <img
              src="{{product.images[0] | img_url : 'original'}}"
              alt="{{product.title}}"
              width="220"
              height="110"
              loading="lazy"
              alt=""
              class="cta_image">
          {% endfor %}
        </div>
        <div id="w-node-d9f7959d-a9b5-9387-cac8-6e7c88cfaee2-732b4aee" class="cta_image-wrapper _2">
          {% for product in collections.all.products %}
            <img
              src="{{product.images[0] | img_url : 'original'}}"
              alt="{{product.title}}"
              width="220"
              height="110"
              loading="lazy"
              alt=""
              class="cta_image">
          {% endfor %}
        </div>
        <div id="w-node-d9f7959d-a9b5-9387-cac8-6e7c88cfaee9-732b4aee" class="cta_image-wrapper _3">
          {% for product in collections.all.products %}
            <img
              src="{{product.images[0] | img_url : 'original'}}"
              alt="{{product.title}}"
              width="220"
              height="110"
              loading="lazy"
              alt=""
              class="cta_image">
          {% endfor %}
        </div>
      </div>
    </div>
  </div>
</section>
{% schema %}
  {
    "name": "Home CTA",
    "blocks": [
      {
        "type": "heading",
        "name": "Heading Block",
        "limit": 1,
        "settings": [
          {
            "type": "text",
            "id": "heading",
            "label": "t:sections.cta-home.blocks.heading.settings.heading.label"
          }, {
            "type": "text",
            "id": "tagline",
            "label": "t:sections.cta-home.blocks.heading.settings.tagline.label"
          }
        ]
      }, {
        "type": "text",
        "name": "CTA Heading",
        "settings": [
          {
            "type": "text",
            "id": "text",
            "label": "t:sections.cta-home.blocks.text.settings.text.label",
            "default": "t:sections.cta-home.blocks.text.settings.text.default"
          }
        ]
      }, {
        "type": "button",
        "name": "CTA Button",
        "settings": [
          {
            "type": "text",
            "id": "cta_button",
            "label": "t:sections.cta-home.blocks.button.settings.text.label",
            "default": "t:sections.cta-home.blocks.button.settings.text.default"
          }, {
            "type": "url",
            "id": "cta_link",
            "label": "t:sections.cta-home.blocks.button.settings.url.label",
            "default": "t:sections.cta-home.blocks.button.settings.url.default"
          }
        ]
      }
    ],
    "presets": [
      {
        "name": "Home CTA",
        "blocks": [
          {
            "type": "heading"
          }, {
            "type": "text"
          }, {
            "type": "button"
          }
        ]
      }
    ]
  }
{% endschema %}