<section class="section_component">
  <div class="container">
    <div class="property-card-wrapper-main">
      <div
        data-w-id="bc17c2cb-6525-3fb7-007c-e2d8a05c1327"
        style="opacity: 1; transform: translate3d(0px, 0px, 0px) scale3d(1, 1, 1) rotateX(0deg) rotateY(0deg) rotateZ(0deg) skew(0deg, 0deg); transform-style: preserve-3d;"
        class="res-offplan-container">
        {%- for block in section.blocks -%}
          {%- case block.type -%}
            {%- when "heading" -%}
              <h2>{{ block.settings.heading }}</h2>

            {%- when "text" -%}
              <div class="description">
                {{ block.settings.description }}
              </div>
          {%- endcase -%}
        {%- endfor -%}
        <div class="prop-developer-items">
          {%- for block in section.blocks -%}
            {%- case block.type -%}
              {%- when "collection" -%}
                <div id="w-node-_6d3470c6-9fae-3ace-1c00-f6f61047757e-732b4aee" class="developer-logo-list">
                  <a href="{{collections[block.settings.dev_collection].url}}" class="content-section is-developer">
                    <img
                      src="{{ collections[block.settings.dev_collection].featured_image | img_url : 'original' }}"
                      loading="lazy"
                      alt="{{ collections[block.settings.dev_collection].title }}"
                      width="100"
                      height="42"
                      class="developer-logo">
                  </a>
                </div>
            {%- endcase -%}
          {%- endfor -%}

        </div>
      </div>
    </div>
  </div>
</section>

{% schema %}
  {
    "name": "Developers",
    "blocks": [
      {
        "type": "heading",
        "name": "Heading",
        "limit": 1,
        "settings": [
          {
            "type": "text",
            "id": "heading",
            "label": "t:sections.developers.blocks.heading.settings.heading.label",
            "default": "t:sections.developers.blocks.heading.settings.heading.default"
          }
        ]
      }, {
        "type": "text",
        "name": "Description",
        "limit": 1,
        "settings": [
          {
            "type": "text",
            "id": "description",
            "label": "t:sections.developers.blocks.text.settings.text.label",
            "default": "t:sections.developers.blocks.text.settings.text.default"
          }
        ]
      }, {
        "type": "collection",
        "name": "Collection Developers",
        "settings": [
          {
            "type": "text",
            "id": "dev_collection",
            "label": "t:sections.developers.blocks.collection.settings.collection.label",
            "default": "t:sections.developers.blocks.collection.settings.collection.default"
          }
        ]
      }
    ],
    "presets": [
      {
        "name": "Developers",
        "blocks": [
          {
            "type": "heading"

          }, {
            "type": "text"

          }, {
            "type": "collection"

          }
        ]
      }
    ]
  }
{% endschema %}