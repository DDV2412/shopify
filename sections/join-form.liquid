<section class="section_component background-grey">
    <div class="container">
        <div class="landing-content-wrapper">
            <div data-w-id="bc17c2cb-6525-3fb7-007c-e2d8a05c13a4"
                style="opacity: 1; transform: translate3d(0px, 0px, 0px) scale3d(1, 1, 1) rotateX(0deg) rotateY(0deg) rotateZ(0deg) skew(0deg, 0deg); transform-style: preserve-3d;"
                class="content-section padding-0">
                <div class="offplan-hero">
                    <div id="w-node-bc17c2cb-6525-3fb7-007c-e2d8a05c13a6-732b4aee" class="hero_content is-offplan">
                        {%- for block in section.blocks -%}
                            {%- case block.type -%}
                                {%- when 'heading' -%}
                                <h1>{{ block.settings.heading }}</h1>

                                {%- when 'text' -%}
                                    <p class="description">{{ block.settings.text }}</p>

                                    <div class="button-wrapper align-left"><a href="https://wa.me/{{ block.settings.phone }}"
                                class="button is-secondary is-contact-icon w-inline-block">
                                <div class="whatsapp-icon is-medium w-embed"><svg xmlns="http://www.w3.org/2000/svg"
                                        width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor"
                                        stroke-width="2" stroke-linecap="round" stroke-linejoin="round"
                                        class="icon icon-tabler icons-tabler-outline icon-tabler-brand-whatsapp">
                                        <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                        <path d="M3 21l1.65 -3.8a9 9 0 1 1 3.4 2.9l-5.05 .9"></path>
                                        <path
                                            d="M9 10a.5 .5 0 0 0 1 0v-1a.5 .5 0 0 0 -1 0v1a5 5 0 0 0 5 5h1a.5 .5 0 0 0 0 -1h-1a.5 .5 0 0 0 0 1">
                                        </path>
                                    </svg></div>
                                <div>Click to Whatsapp</div>
                            </a></div>
                        <div class="button-wrapper align-left"><a href="mailto:{{ block.settings.email }}"
                                class="button is-secondary is-contact-icon w-inline-block">
                                <div class="whatsapp-icon is-medium w-embed"><svg xmlns="http://www.w3.org/2000/svg"
                                        width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor"
                                        stroke-width="2" stroke-linecap="round" stroke-linejoin="round"
                                        class="icon icon-tabler icons-tabler-outline icon-tabler-mail">
                                        <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                        <path
                                            d="M3 7a2 2 0 0 1 2 -2h14a2 2 0 0 1 2 2v10a2 2 0 0 1 -2 2h-14a2 2 0 0 1 -2 -2v-10z">
                                        </path>
                                        <path d="M3 7l9 6l9 -6"></path>
                                    </svg></div>
                                <div>{{ block.settings.email }}</div>
                            </a></div>
                                
                            {%- endcase -%}
                            {%- endfor -%}
                    </div>
                    <div id="w-node-bc17c2cb-6525-3fb7-007c-e2d8a05c13b7-732b4aee" class="hero_content">
                        <div class="join-block w-form">
                            <form id="email-form-4" name="email-form-4" data-name="Email Form 4" method="get"
                                class="form" data-wf-page-id="66eaa6ae2a24a29c732b4aee"
                                data-wf-element-id="bc17c2cb-6525-3fb7-007c-e2d8a05c13b9" aria-label="Email Form 4">
                                <div class="grid-2">
                                    <div id="w-node-bc17c2cb-6525-3fb7-007c-e2d8a05c13bb-732b4aee"
                                        class="search-wrapper background-white is-form"><input
                                            class="search-field w-input" maxlength="256" name="First Name"
                                            data-name="First Name" placeholder="First Name" type="text"
                                            id="First-Name"></div>
                                    <div id="w-node-bc17c2cb-6525-3fb7-007c-e2d8a05c13bd-732b4aee"
                                        class="search-wrapper background-white is-form"><input
                                            class="search-field w-input" maxlength="256" name="Last Name"
                                            data-name="Last Name" placeholder="Last Name" type="text"
                                            id="Last-Name"></div>
                                </div>
                                <div class="grid-1">
                                    <div id="w-node-bc17c2cb-6525-3fb7-007c-e2d8a05c13c0-732b4aee"
                                        class="search-wrapper background-white is-form"><input
                                            class="search-field w-input" maxlength="256" name="Email Address"
                                            data-name="Email Address" placeholder="Email Address" type="text"
                                            id="Email-Address">
                                            </div>
                                </div>
                                <div class="grid-1">
                                    <div id="w-node-bc17c2cb-6525-3fb7-007c-e2d8a05c13c3-732b4aee"
                                        class="search-wrapper background-white is-form"><input
                                            class="search-field w-input" maxlength="256" name="Phone Number"
                                            data-name="Phone Number" placeholder="Phone Number" type="tel"
                                            id="Phone-Number"></div>
                                </div>
                                <div class="button-wrapper padding-top-large is-form"><input type="submit"
                                        data-wait="Please wait..." class="button is-form w-button" value="Submit">
                                    <div class="terms-and-privacy text-primary">By clicking Submit, you agree to our <a
                                            target="_blank" href="{{ settings.terms_of_service }}"
                                            class="terms-and-privacy text-primary">Terms &amp; Conditions </a>and<a
                                            target="_blank" href="{{ settings.privacy_policy }}"
                                            class="terms-and-privacy text-primary"> Privacy Policy </a>.</div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>


{% schema %}
  {
    "name": "Join Form",
    "blocks": [
      {
        "type": "heading",
        "name": "Heading Block",
        "limit": 1,
        "settings": [
          {
            "type": "text",
            "id": "heading",
            "label": "t:sections.join-form.blocks.heading.settings.heading.label"
          }
        ]
      }, {
        "type": "text",
        "name": "Text Block",
        "limit": 1,
        "settings": [
          {
            "type": "text",
            "id": "text",
            "label": "t:sections.join-form.blocks.text.settings.text.label",
            "default": "t:sections.join-form.blocks.text.settings.text.default"
          }, {
            "type": "text",
            "id": "email",
            "label": "t:sections.join-form.blocks.text.settings.email.label",
            "default": "t:sections.join-form.blocks.text.settings.email.default"
          }, {
            "type": "text",
            "id": "phone",
            "label": "t:sections.join-form.blocks.text.settings.phone.label",
            "default": "t:sections.join-form.blocks.text.settings.phone.default"
          }
        ]
      }

    ],
    "presets": [
      {
        "name": "Join Form",
        "blocks": [
          {
            "type": "heading"
          }, {
            "type": "text"
          }
        ]
      }
    ]
  }
{% endschema %}